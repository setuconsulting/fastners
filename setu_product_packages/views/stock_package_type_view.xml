<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="setu_stock_package_type_form_view" model="ir.ui.view">
            <field name="name">setu.stock.package.type.form.view</field>
            <field name="model">stock.package.type</field>
            <field name="inherit_id" ref="stock.stock_package_type_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="prefix"/>
                    <field name="package_product_id"/>
                    <field name="is_visible_package_qty"/>
                    <field name="is_visible_package_weight"/>
                    <field name="is_outer_box"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
